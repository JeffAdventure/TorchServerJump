<UserControl x:Class="ServerJump.JumpGateControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ServerJump"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">


    <Grid HorizontalAlignment="Left" Height="300" VerticalAlignment="Top" Width="300">
        <TextBox x:Name="PassTextBox" Margin="10,31,160,229" Text="{Binding Password}" Height="40" />
        <Button x:Name="SaveBtn" Content="Save" HorizontalAlignment="Left" VerticalAlignment="Top" Width="130" Margin="10,70,0,0" Click="SaveBtn_Click" Height="33"/>
        <Label Content="Password" Margin="145,31,31,0" Height="31" VerticalAlignment="Top" />
        <CheckBox Content="Enable Link" IsChecked="{Binding Enabled}" HorizontalAlignment="Left" Height="19" Margin="22,10,0,0" VerticalAlignment="Top" Width="129"/>
    </Grid>
</UserControl>
